                        <h4><%=list[i].name%></h4>
                    </div>
                    <span id="spid" data-bid="<%=list[i].userId%>" style="display: none"></span>
                    <button class='F_btn cel_btn' onclick="fans(this)">关注</button>
                </div>
            <%}%>
        </div>
        <div class="split_page">
