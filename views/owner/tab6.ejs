                    </div>
                    <div class='fans_font'>
                        <h4><%=list[i].name%></h4>
                    </div>
                    <button data-bid="<%=list[i].userId%>" class='F_btn cel_btn' onclick="fans(this)">关注</button>
                </div>
            <%}%>
        </div>
       <!-- 分页 --> 
        <div class="fenye" style="text-align:center;">
            <ul data-userid="<%=sessionid%>" data-name="6" data-pagecount="<%=page.total%>" data-pagesize="<%=page.size%>" data-current="<%=page.current%>" class="pagination" id="pagination"></ul>
